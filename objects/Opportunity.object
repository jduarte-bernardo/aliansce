    <webLinks>
        <fullName>Nova_CotacaoJS</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Nova Cotação</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)} 

var profile = &apos;{!$Profile.Name}&apos;;
var estagio = &apos;{!Opportunity.StageName}&apos;;

if(profile == &apos;Executivo de vendas&apos; &amp;&amp; estagio == &apos;Aguardando aprovação&apos;)
{
  alert(&apos;Não é possível criar uma cotação com a oportunidade no estagio: Aguardando aprovação &apos;);
}else{
