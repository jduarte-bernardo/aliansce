    <validationRules>
        <fullName>TW1_OportunidadeEvNaoAlteraFaseVendas</fullName>
        <active>false</active>
        <description>Impede que o EV altere o estágio de vendas da oportunidade com valores referentes a aprovação e integração.</description>
        <errorConditionFormula>AND( 
        OR(ISPICKVAL(StageName, &apos;Aprovada&apos;), 
              ISPICKVAL(StageName, &apos;Integrada&apos;) , 
              ISPICKVAL(StageName, &apos;Erro de integração&apos;), 
              ISPICKVAL(StageName,&apos;Rejeitada&apos;)
             ), 
         OR($User.ProfileId = &apos;00e0R000000IHSK&apos;, $User.ProfileId = &apos;00e0R000000Lum2&apos;),
         ISCHANGED(StageName) 
)</errorConditionFormula>
        <errorMessage>Você não tem permissão para alterar o estágio da proposta para o valor selecionado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_OportunidadeEvitaEdicaoSeIntegrada</fullName>
        <active>false</active>
        <description>Evita que uma oportunidade com estágio &quot;Integrada&quot; seja alterada por usuários com perfil diferente de Admin.</description>
        <errorConditionFormula>AND( 
        OR( 
              AND(ISCHANGED(StageName), ISPICKVAL(PRIORVALUE(StageName),&apos;Integrada&apos;)), 
              ISPICKVAL(StageName,&apos;Integrada&apos;) 
             ), 
       NOT($Profile.Id = &apos;00e1I000000xiXe&apos;) 
)</errorConditionFormula>
        <errorMessage>Esta oportunidade não pode ser alterada pois já foi integrada.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_OportunidadeExigeMarca</fullName>
        <active>false</active>
        <description>Exige o preenchimento do campo marca ou campo marca pretendida.</description>
        <errorConditionFormula>AND(ISBLANK(TW1_Marca__c),ISBLANK(TW1_Marca_pretendida__c), NOT(TW1_OpportunityRecordTypeName__c = &quot;Oportunidade de Feiras e Eventos&quot;))</errorConditionFormula>
        <errorMessage>Favor preencher o campo Marca ou o campo Marca pretendida.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_OportunidadeIntegracaoSemAprovacao</fullName>
        <active>false</active>
        <description>Impede que o usuário envie a oportunidade para integração sem a aprovação do gerente regional.</description>
        <errorConditionFormula>AND(ISCHANGED(StageName),ISPICKVAL(StageName, &apos;Enviada para Integração&apos;) ,TW1_Oportunidade_aprovada__c = False)</errorConditionFormula>
        <errorMessage>A oportunidade só pode ser enviada para integração se estiver aprovada.</errorMessage>
    </validationRules>
