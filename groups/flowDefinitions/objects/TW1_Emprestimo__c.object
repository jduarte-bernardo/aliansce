    <validationRules>
        <fullName>TW1_VR_Emprestimo_Solicita_Data_Devolu</fullName>
        <active>false</active>
        <description>Se o status for alterado para devolvido o sistema solicita data de devolução.</description>
        <errorConditionFormula>ISCHANGED(TW1_Status__c) &amp;&amp; ISPICKVAL(TW1_Status__c, &apos;Devolvido&apos;) &amp;&amp; ISNULL(TW1_Data_da_Devolucao__c)</errorConditionFormula>
        <errorMessage>Favor preencher data de devolução.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_VR_Emprestimo_Solicita_status</fullName>
        <active>false</active>
        <description>A data de devolução só deve ser preenchida com o status igual a devolvida.</description>
        <errorConditionFormula>NOT(ISNULL(TW1_Data_da_Devolucao__c)) &amp;&amp; ISPICKVAL(TW1_Status__c,&quot;Em uso&quot;)</errorConditionFormula>
        <errorMessage>Se a data de devolução for preenchida alterar o status para Devolvido.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_VR_Emprestimo_Valida_status_item</fullName>
        <active>false</active>
        <description>Verifica se o item selecionado está com o status de Locado, caso positivo impede o salvamento solicitando um item vago.</description>
        <errorConditionFormula>(ISNEW() || ISCHANGED(TW1_Item__c)) &amp;&amp; ISPICKVAL(TW1_Item__r.TW1_Status__c, &apos;Locado&apos;)</errorConditionFormula>
        <errorMessage>Não é possível vincular um item com status de locado a um empréstimo já existente, por favor selecione um item vago.</errorMessage>
    </validationRules>
